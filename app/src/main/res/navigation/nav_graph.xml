<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.surfind.HomeFragment"
        android:label="SurFind"
        tools:layout="@layout/fragment_home"/>
    <fragment
        android:id="@+id/spotsListFragment"
        android:name="com.example.surfind.SpotsListFragment"
        android:label="Spots List"
        tools:layout="@layout/fragment_spots_list" />
    <fragment
        android:id="@+id/spotReportListFragment"
        android:name="com.example.surfind.SpotReportsListFragment"
        android:label="Reports List"
        tools:layout="@layout/fragment_spot_reports_list" >
        <argument
            android:name="spot"
            app:argType="com.example.surfind.model.Spot" />
    </fragment>
    <action android:id="@+id/action_global_coastsListFragment" app:destination="@id/spotsListFragment" /><action android:id="@+id/action_global_coastReportListFragment" app:destination="@id/spotReportListFragment"/>
</navigation>